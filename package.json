{
  "name": "airtable-classroom",
  "version": "1.0.0",
  "description": "a digital classroom template using airtable as the CMS",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o docs",
    "deploy": "npm run build && git add -A && git commit -m 'update' && git push",
    "css:build": "node-sass --omit-source-map-url assets/sass/styles.scss public/css/styles.css",
    "css:watch": "npm run css:build -- --watch"
  },
  "author": "jollygrin",
  "license": "MIT",
  "dependencies": {
    "airtable": "^0.10.0",
    "next": "^9.5.3",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
